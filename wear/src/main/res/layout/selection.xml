<android.support.wearable.view.BoxInsetLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:id="@+id/what_layout"

    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:padding="1dp"    >
    <!--android:padding="30dp"-->

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:keepScreenOn="true"
        android:background="@color/white"
        app:layout_box="all">



        //TextView Auswahl treffen
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="87dp"
            android:text="@string/textview_what"
            android:id="@+id/textview_mitarbeiter_auswahl"
            android:layout_gravity="center_horizontal|center"
            android:textColor="#000"/>


        // Button Springer
        <Button
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:text="@string/buttonselection"
            android:id="@+id/buttonselection"
            android:layout_gravity="center"
            android:onClick="OnClickSelection"/>

        // Button Experte
        <Button
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:text="@string/delayedinformation"
            android:id="@+id/buttondelayedinformation"
            android:layout_gravity="center_horizontal|bottom"
            android:onClick="OnClickDelayedInformation"/>






    </FrameLayout>

</android.support.wearable.view.BoxInsetLayout>



